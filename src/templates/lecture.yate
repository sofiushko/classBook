module "lecture"

match / {
    <a class = "link lecture__title" href="#/lectures/{ .id }">{ .name }</a>
    if (.lector.attributes.name) {
        <a href = "#/lectors/{.lector.attributes.id}" class = "lecture__lector">
            <p class="lecture__lector-name">{ .lector.attributes.name }</p>
        </a>
    }
    if .lecture_date {
        <p class="lecture_date">{ .lecture_date }</p>
    }
}